package io.sarl.sre.tests.runtime.bugs.bug66

import io.sarl.lang.core.SpaceID
import io.sarl.util.OpenEventSpace
import io.sarl.lang.util.SynchronizedSet
import java.util.UUID
import io.sarl.util.Collections3

package class MySpaceImpl implements MySpace {

	val id : SpaceID

	val otherSpace : OpenEventSpace
	
	new (id : SpaceID, otherSpace : OpenEventSpace) {
		this.id = id
		this.otherSpace = otherSpace
	}
	
	override getTheOtherSpace : OpenEventSpace {
		this.otherSpace
	}

	override getSpaceID : SpaceID {
		this.id
	}

	override getParticipants : SynchronizedSet<UUID> {
		Collections3::emptySynchronizedSet
	}

}
